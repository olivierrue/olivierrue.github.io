		Lien(s) = {http://rdcu.be/zfj4, https://link.springer.com/epdf/10.1186/s13326-017-0165-6?author_access_token=3LeJeiU6sqtiRtPPA1WyLW_BpE1tBhCbnbw3BuzI2RP3kBlYIyGWXKvd9k04Q8iibmLfsC6EzEW9lyA9d7qpkvTmUE07su1GGfdahhB9sY0b0pUyZTxWb9XealXgvCFp92Nu4P4a6ydMItuiYBCsHw%3D%3D},
		Contact = {Anne Goelzer},
		Affiliation auteur = {Institut National de la Recherche Agronomique, Université Paris Sud (Paris 11), , Université Paris Sud (Paris 11), , , , Université Paris Sud (Paris 11), },
