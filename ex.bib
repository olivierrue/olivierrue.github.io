		Langue du résumé = {eng},
		Pays de l'unité = {FRA, },
		Clé UT (WOS) = {000338607100007},
		Type d'article = {Research article},
		Mot clé chercheur = {Lifespan, evolution, longevity, mitochondria, mitochondrially-encoded protein, mtDNA},
		Comité de lecture = {true},
